packages: .

package swapi
  optimization: 2
  executable-static: True
  split-sections: True
  executable-stripping: True
  ghc-options: -threaded -rtsopts -with-rtsopts=-N -optl-pthread -fPIC

package text
  flags: +integer-simple

package hashable
  flags: -integer-gmp

package scientific
  flags: +integer-simple

package integer-logarithms
  flags: -integer-gmp

package cryptonite
  flags: -integer-gmp

package text-show
  flags: -integer-gmp
